config_version=4

_global_script_classes=[ {
"base": "Resource",
"class": "DialogueResource",
"language": "GDScript",
"path": "res://addons/dialogue_manager/dialogue_resource.gd"
} ]
_global_script_class_icons={
"DialogueResource": ""
}

[application]

config/name="Zombie Rush"
run/main_scene="res://Scenes/TestScene.tscn"
run/delta_sync_after_draw=true
config/icon="res://icon.png"

[autoload]

GlobalData="*res://Scripts/Helper/GlobalData.cs"
DialogueManager="*res://addons/dialogue_manager/dialogue_manager.gd"
LevelManager="*res://Scripts/Managers/LevelManager.cs"
PlayerManager="*res://Objects/Managers/PlayerManager.tscn"

[display]

window/vsync/use_vsync=false

[editor_plugins]

enabled=PoolStringArray( "res://addons/dialogue_manager/plugin.cfg", "res://addons/vnen.tiled_importer/plugin.cfg" )

[global]

snap=false

[gui]

common/drop_mouse_on_gui_input_disabled=true

[importer_defaults]

texture={
"compress/mode": 3,
"flags/filter": false
}
vnen.tiled_importer={
"add_background": true,
"collision_layer": 1,
"custom_properties": true,
"embed_internal_images": true,
"image_flags": 0,
"post_import_script": "",
"save_tiled_properties": false,
"tile_metadata": false,
"uv_clip": true
}

